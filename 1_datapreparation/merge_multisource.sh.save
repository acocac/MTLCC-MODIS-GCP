#!/bin/bash

#YEARS=(2004 2005 2006 2007 2008 2009 2010 2011 2012 2013 2014 2015 2016 2017)
YEARS=(2015)
project=(tile_1_612)
psize=(384)
timesteps=(46)

mkdir -p "/home/quangthang_fami53/data/test_coca/${project}/_logs"

for year in ${YEARS[@]}; do
    echo "Processing $year"
    logfname="/home/quangthang_fami53/data/test_coca/${project}/_logs/$year.log"
    python3 0_merge_multisources.py \
        --rootdir="/home/quangthang_fami53/data/test_coca/${project}" \
        --psize=$psize \
        --tyear=$year \
        --timesteps=$timesteps > $logfname 2>&1

done
